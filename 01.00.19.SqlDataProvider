ALTER procedure [dbo].[GIBS_GiftCertDeleteGiftCert]

	@ModuleId       int,
        @ItemId         int

as

delete
from   GIBS_GiftCertificate
where  ModuleId = @ModuleId
and    ItemId = @ItemId
GO

