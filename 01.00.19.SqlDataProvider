ALTER procedure [dbo].[GIBS_GiftCertGetGiftCert]

	
        @ItemId int

as

select [ItemID]
      ,[CertAmount]
      ,[MailTo]
      ,[ToName]
      ,[MailToAddress]
      ,[MailToAddress1]
      ,[MailToCity]
      ,[MailToState]
      ,[MailToZip]
      ,[FromUserID]
      ,[FromName]
      ,[FromPhone]
      ,[FromEmail]
      ,[Notes]
      ,[GIBS_GiftCertificate].[CreatedByUserID]
      ,[UpdatedByUserID]
      ,[CreatedDate]
      ,[isProcessed]
      ,[PP_PaymentId]
      ,[PP_Response]
      ,[UpdateDate]
	  ,PaypalPaymentState
	  ,  'CreatedByUserName' = Users.FirstName + ' ' + Users.LastName

from [GIBS_GiftCertificate]
left outer join  Users on [GIBS_GiftCertificate].CreatedByUserID = Users.UserId
where  ItemId = @ItemId
GO

