END
GO

-- PP_PaymentId, PP_Response, UpdateDate, PaypalPaymentState



ALTER procedure [dbo].[GIBS_GiftCertUpdateGiftCert]
(  
    @ModuleID	int,
    @ItemID int,
   	@CertAmount money,
	@MailTo [varchar](50),
	@ToName [varchar](50),
	@ToAddress [varchar](50),
	@ToAddress1 [varchar](50),
	@ToCity [varchar](50),
	@ToState [varchar](2),
	@ToZip [varchar](11),

	@FromName [varchar](50),
	@FromPhone [varchar](50),
	@FromEmail [varchar](50),
	
	@Notes        [nvarchar](max),
	@UpdatedByUserID	int,
	@IsProcessed	bit
      
)
as

BEGIN 
      SET NOCOUNT ON;

	  DECLARE @NewLineChar AS CHAR(2) = CHAR(13) + CHAR(10)

	  UPDATE [dbo].[GIBS_GiftCertificate]
   SET 
   [CertAmount] = @CertAmount
      ,[MailTo] = @MailTo
      ,[ToName] = @ToName
      ,[MailToAddress] = @ToAddress
      ,[MailToAddress1] = @ToAddress1
      ,[MailToCity] = @ToCity
      ,[MailToState] = @ToState
      ,[MailToZip] = @ToZip
     
      ,[FromName] = @FromName
      ,[FromPhone] = @FromPhone
      ,[FromEmail] = @FromEmail
      ,[Notes] = @Notes

      ,[isProcessed] = @IsProcessed
      
      ,[UpdateDate] = GetDate()
	  ,UpdatedByUserID = @UpdatedByUserID

      where itemid=@ItemID AND ModuleID = @ModuleID
end
GO

