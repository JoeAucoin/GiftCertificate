ALTER procedure [dbo].[GIBS_GiftCertAddGiftCert]

	@ModuleId       int,
	@CertAmount money,
	@MailTo [varchar](50),
	@ToName [varchar](50),
	@ToAddress [varchar](50),
	@ToAddress1 [varchar](50),
	@ToCity [varchar](50),
	@ToState [varchar](2),
	@ToZip [varchar](11),
	@FromName [varchar](50),
	@FromUserID       int,
	@FromPhone [varchar](50),
	@FromEmail [varchar](50),
	@Notes        ntext,
	@CreatedByUserID         int

as

BEGIN 
	SET NOCOUNT ON;

INSERT INTO GIBS_GiftCertificate
           ([ModuleID]
           ,[CertAmount]
           ,[MailTo]
           ,[ToName]
           ,[MailToAddress]
           ,[MailToAddress1]
           ,[MailToCity]
           ,[MailToState]
           ,[MailToZip]
           ,[FromUserID]
           ,[FromName]
           ,[FromPhone]
           ,[FromEmail]
           ,[Notes]
           ,[CreatedByUserID]
           ,[UpdatedByUserID]
           ,[CreatedDate]
		   )
 
values (
	@ModuleId,
	@CertAmount,
	@MailTo,
	@ToName,
	@ToAddress,
	@ToAddress1,
	@ToCity,
	@ToState,
	@ToZip,
	@FromUserID,
	@FromName,

	@FromPhone,
	@FromEmail,

	@Notes,
	@CreatedByUserID,
	@CreatedByUserID,
	getdate()
)

select SCOPE_IDENTITY() 

end
GO

